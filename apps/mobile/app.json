{
  "expo": {
    "name": "Buzzreel",
    "slug": "buzzreel",
    "version": "2.0.0",
    "orientation": "portrait",
    "platforms": [
      "ios",
      "android",
      "web"
    ],
    "sdkVersion": "54.0.0",
    "icon": "./assets/icon.png",
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/icon.png",
        "backgroundColor": "#0c0d12"
      },
      "package": "com.buzzreel.app",
      "versionCode": 8,
      "permissions": [
        "com.google.android.gms.permission.AD_ID",
        "com.android.vending.BILLING"
      ],
      "config": {
        "googleMobileAdsAppId": "ca-app-pub-1580761947831808~4467016439"
      }
    },
    "ios": {
      "bundleIdentifier": "com.buzzreel.app",
      "buildNumber": "2",
      "supportsTablet": true,
      "infoPlist": {
        "SKAdNetworkItems": [
          {
            "SKAdNetworkIdentifier": "cstr6suwn9.skadnetwork"
          }
        ]
      },
      "config": {
        "googleMobileAdsAppId": "ca-app-pub-1580761947831808~4467016439"
      }
    },
    "plugins": [
      "./plugins/withDisableCppWarningsAsErrors",
      [
        "expo-build-properties",
        {
          "android": {
            "newArchEnabled": false,
            "kotlinVersion": "2.2.0",
            "compileSdkVersion": 35,
            "targetSdkVersion": 35,
            "extraProguardRules": "-dontwarn com.swmansion.rnscreens.**"
          },
          "ios": {
            "newArchEnabled": false
          }
        }
      ],
      [
        "react-native-google-mobile-ads",
        {
          "androidAppId": "ca-app-pub-1580761947831808~4467016439",
          "iosAppId": "ca-app-pub-1580761947831808~4467016439"
        }
      ],
      "expo-iap",
      "./plugins/withAdIdPermission"
    ],
    "extra": {
      "eas": {
        "projectId": "08d75255-db2f-4ca3-81e7-9d60a9d69708"
      }
    },
    "owner": "waypointsolutions2023"
  }
}
